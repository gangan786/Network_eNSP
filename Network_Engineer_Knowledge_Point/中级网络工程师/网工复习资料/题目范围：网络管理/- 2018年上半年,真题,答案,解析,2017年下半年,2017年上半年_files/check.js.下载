}



function showdaohang(product_id,url,paper_id,tixing,daohang_type,daohang_kemu_id){
//	alert(tixing);
	var web_url = "";
	if (daohang_type == 0)
	{
		web_url = url + "/tk_th_daohang.jsp?product_id=" + product_id + "&paper_id=" + paper_id + "&tixing=" + tixing + "&daohang_type=" + daohang_type + "&daohang_kemu_id=" + daohang_kemu_id + ""; 
	}else if(daohang_type == 1 || daohang_type == 2){
		web_url = url + "/tk_th_daohang_my.jsp?product_id=" + product_id + "&paper_id=" + paper_id + "&tixing=" + tixing + "&daohang_type=" + daohang_type + "&daohang_kemu_id=" + daohang_kemu_id + ""; 	
	}

	$.get(web_url, {cache:false},function (data, textStatus){
		document.getElementById('th_daohang_area').style.display='block';
		document.getElementById("th_daohang_area").innerHTML = data;
	});
}


function save_tj_type(tj_type,answer_time){
	document.getElementById('tj_type').value = tj_type;

	var answer_time_str = answer_time.split(" ");
	var shi = parseInt(answer_time_str[0]);
	var fen = parseInt(answer_time_str[1]);
	var miao = parseInt(answer_time_str[2]);
	var answer_time_haomiao = parseInt(answer_time_str[0]) * 3600 + parseInt(answer_time_str[1]) * 60 + parseInt(answer_time_str[2]);
	document.getElementById('tj_answer_time').value = answer_time_haomiao;


}



function changeaction(type){
//	alert(sheet_list);
//	alert(tj_type);


	if (type == 1 || type == 2)
	{
		document.forms["form_chongci_fufei"].action = "../dealwithcenter.jsp?action=tk_fufei_kt";
	}
	else if (type == 3)//
	{
		document.forms["form_chongci_fufei"].action = "../dealwithcenter.jsp?action=tk_chongci_kt";
	}


}


function tijiao_zice(sheet_list,tj_type){
//	alert(sheet_list);
//	alert(tj_type);




	if (tj_type == 2)//暂停
	{
//		window.location="tk_zice_paper_list.jsp";
		document.forms["form_zice_paper"].action = "tk_my_zice.jsp";


	}else if (tj_type == 1)//提交
	{
		var finish = 1;
		var sheet_list_str = sheet_list.split("_");
		for (i = 0;i < sheet_list_str.length ;i ++ )
		{
			if(sheet_list_str[i] != "")
			{	
				var sheet_question_str = sheet_list_str[i].split("|");
			//	alert(sheet_list_str[i]);
				if (sheet_question_str[1] == 0)
				{
					finish = 0;
				}			
			}
		}
		var tishi = "";
		if (finish == 0)
		{
			tishi = "还有未做完试题，您确定提交试卷吗？";
		}else if(finish == 1){
			tishi = "您确定提交试卷吗？";		
		}


		
		if (confirm(tishi))
		{
			return true;
			
		}else{
			return false;	
		}
	}


}


function start_zujuan(fw,timu,cnt,tihao_shunxu,xuanxiang_shunxu,time,kemu_selected,ruanfan,user_id){
	//用户是否登录
	//检查是否有软饭功能被选了 如果有提示
	//检查内容是否选全了
	
//	alert();

	if(user_id == 0){//用户没有登录
		document.getElementById('text_area_2').style.display='block';
		return false;
	}

	//先关掉有可能上次点击鼠标时候的弹出提示
	document.getElementById('text_area_6').style.display='none';
	//检查是否有软饭功能被选了 如果有提示
	if (ruanfan == 0 && (fw == 6 || xuanxiang_shunxu == 2))//非软饭会员 选择了软饭的选项
	{
	//	alert();
		document.getElementById('text_area_6').style.display='block';
		return false;
	}
	//检查内容是否选全了
	if (fw == 5)
	{
		if (timu == '')
		{
			alert('请选择分类范围');
			return false;
		}
	}
	if (fw == 6)
	{
		if (timu == '')
		{
			alert('请选择星级范围');
			return false;
		}
	}

	//如果都没问题了 就开始提交 进入下一步了
	if (confirm("您确定要按所选方式开始组卷吗？"))
	{
//		alert();
		return true;
	}else{
		return false;
	}


}

