function chkUpChengji(){
	var kaojimen = document.getElementById("kaojimen").value;
//	alert(kaojimen);

	if (document.getElementById("chengji_sw").value == "")
	{
		alert("请选择上午成绩");
		return(false);
	}else if(document.getElementById("chengji_xw_a").value == "")
	{
		alert("请选择下午成绩一");
		return(false);
	}else if(kaojimen == 3 && document.getElementById("chengji_xw_b").value == "")
	{
		alert("请选择下午成绩二");
		return(false);
	}else{
		if (confirm("确定吗？上传之后，将不能修改。"))
		{		
			return(true);
		}else{
			return(false);		
		}	
	
	}



}

function chkTkZuocuolv(){


	var f = document.forms["form1"]; 
	var checkbox_list = "";
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name == "nandu"){
			if (f.elements[i].checked == true)
			{
				checkbox_list += "," + f.elements[i].value + ",";			
			}
		}		
	}
	document.getElementById("checkbox_list").value = checkbox_list;




	var select_kemu_id = document.getElementById("select_kemu_id").value;
//	alert(tk_keywords);
	if (select_kemu_id == 0)
	{
		alert("请选择科目");
		return(false);
	}



}







function ZujiLeixing(leixing){

	var f = document.forms["form1"]; 
	var selected = 0;
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="leixing" && f.elements[i].checked == true  && f.elements[i].value == leixing)
		{
			selected = 1;
		}
	}
	//1 被选中 0 没选中

	var name_str = "leixing_" + leixing + "_str";
	var name_str_value = document.getElementById(name_str).value;
	var aList = name_str_value.split(",");
	for (i = 0;i < aList.length ;i ++ )
	{
		if(aList[i] != "")
		{
			if(selected == 1){
				document.getElementById(aList[i]).style.display = 'block';			
			}else{
				document.getElementById(aList[i]).style.display = 'none';						
			}

		}
	}


}


function ZujiZuocuolv(nandu){

	var f = document.forms["form1"]; 
	var selected = 0;
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="nandu" && f.elements[i].checked == true  && f.elements[i].value == nandu)
		{
			selected = 1;
		}
	}
	//1 被选中 0 没选中

	var name_str = "nandu_" + nandu + "_str";
	var name_str_value = document.getElementById(name_str).value;
	var aList = name_str_value.split(",");
	for (i = 0;i < aList.length ;i ++ )
	{
		if(aList[i] != "")
		{
			var nn = "zuocuolv_" + aList[i];
			if(selected == 1){
				document.getElementById(nn).style.display = 'block';			
			}else{
				document.getElementById(nn).style.display = 'none';						
			}

		}
	}


}







function DownloadTkSearch(web_url,tk_keywords){
//	alert();
	var f = document.forms["form1"]; 
//	var tk_keywords = document.getElementById("tk_keywords").value;
	var select_kemu_id = document.getElementById("select_kemu_id").value;
//	alert(tk_keywords);
//	alert(select_kemu_id);

	var leixing_checkbox_list = "";
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="leixing" && f.elements[i].checked == true)
		{
			leixing_checkbox_list += f.elements[i].value + "_";
		}
	}
//	alert(tk_keywords);
	window.open(web_url + "/tk_list_download_1.jsp?tk_keywords=" + tk_keywords + "&kemu_id=" + select_kemu_id + "&leixing_checkbox_list=" + leixing_checkbox_list + "&docu_type=tksearch_list");

}












function chkWdsy(){
//	alert();
	var wdsy_keywords = document.getElementById("wdsy_keywords").value;


	if (wdsy_keywords == "")
	{
		alert("请输入溯源编码");
		return(false);
	}else
	{
		if (wdsy_keywords.length != 17)
		{
			alert("溯源编码应为17位，请重新输入");
			return(false);
		}
	
	
	}


}









function chkTkSearch(){
	var tk_keywords = document.getElementById("tk_keywords").value;
	var select_kemu_id = document.getElementById("select_kemu_id").value;

//	alert(tk_keywords.length);

	if (tk_keywords == "")
	{
		alert("请填写关键词");
		return(false);
	}else
	{
		if (tk_keywords.length > 100)
		{
			alert("关键词不要多于100个字，请精简");
			return(false);
		}
	
	
	}
	if (select_kemu_id == 0)
	{
		alert("请选择科目");
		return(false);
	}



}



