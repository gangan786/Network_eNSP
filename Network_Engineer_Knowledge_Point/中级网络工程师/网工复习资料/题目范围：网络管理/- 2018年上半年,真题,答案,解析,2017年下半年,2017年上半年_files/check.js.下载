function show_kexuan(plan_id,kemu_id,re,zhekou){
	var uu = document.getElementById("kexuan_selected").value;
	var show_daixuan = document.getElementById("show_daixuan").value;
	var task_id = document.getElementById("task_id").value;

	var web_url = "plan_step5_kexuan_xml.jsp?plan_id=" + plan_id + "&kemu_id=" + kemu_id + "&selected_product_id=" + uu + "&re=" + re + "&show_daixuan=" + show_daixuan + "&zhekou=" + zhekou + "&task_id=" + task_id + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
		document.getElementById("kexuan_area").innerHTML = data;
		if (re == 1)
		{
			document.getElementById("addcar_area").style.display = 'none';
			document.getElementById("rebuild_area").style.display = 'block';
		}
	});
//	alert();
}

function ref_kexuan(nn,product_id,plan_id,kemu_id,zhekou,product_cn){
	//nn=1的时候，是选中的，传过来的意思是取消选中
	//nn=2的时候，是没选中的，传过来的意思是选中
	var pp = product_id + ",";
	var kexuan_selected = document.getElementById("kexuan_selected").value;
//	alert(kexuan_selected);
	
	var kk = product_cn + ",";
	var kexuan_selected_cn = document.getElementById("kexuan_selected_cn").value;
//	alert(kexuan_selected_cn);

	if (nn == 1)
	{
		kexuan_selected = kexuan_selected.replace(pp,"");
		kexuan_selected_cn = kexuan_selected_cn.replace(kk,"");
	}else if(nn == 2){
		kexuan_selected += pp;
		kexuan_selected_cn += kk;
	}
	document.getElementById("kexuan_selected").value = kexuan_selected;
	document.getElementById("kexuan_selected_cn").value = kexuan_selected_cn;

	show_kexuan(plan_id,kemu_id,1,zhekou);
}



function ref_kexuan_daixuan(plan_id,kemu_id,zhekou){
	document.getElementById("show_daixuan").value = 1;
	show_kexuan(plan_id,kemu_id,0,zhekou);
}




