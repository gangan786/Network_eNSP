function show_tihao_user_old(paper_id,tixing,url){

//	alert(document.getElementById("tihao_5").innerHTML);
//	alert();

	//显示做错的次数
	var web_url = url + "/tk_paper_tihao_show.jsp?paper_id=" + paper_id + "&tixing=" + tixing + "&type=wrong"; 
	$.get(web_url, {cache:false},function (data, textStatus){
		data = data.replace(/[ ]/g,"");//去空格
		data = data.replace(/[\r\n]/g,"");//去回车
		var aList = data.split(";");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				var blist = aList[i].split(",");
				var name = "wrong_" + blist[0];
				document.getElementById(name).innerHTML = "<span class=red-text>" + blist[1] + "</span>";
			}
		}
	});

	//显示做每个题的次数
	web_url = url + "/tk_paper_tihao_show.jsp?paper_id=" + paper_id + "&tixing=" + tixing + "&type=done"; 
	$.get(web_url, {cache:false},function (data, textStatus){
		data = data.replace(/[ ]/g,"");//去空格
		data = data.replace(/[\r\n]/g,"");//去回车
		var aList = data.split(";");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				var blist = aList[i].split(",");
				var name = "done_" + blist[0];
				document.getElementById(name).innerHTML = "<span class=hui>" + blist[1] + "</span>";

				var name_b = "tihao_" + blist[0];
				document.getElementById(name_b).style.background = "#e4e4e4";
			}
		}
	});

	//显示有标记的小红旗
	web_url = url + "/tk_paper_tihao_show.jsp?paper_id=" + paper_id + "&tixing=" + tixing + "&type=biaoji"; 
	$.get(web_url, {cache:false},function (data, textStatus){
		data = data.replace(/[ ]/g,"");//去空格
		data = data.replace(/[\r\n]/g,"");//去回车
		var aList = data.split(";");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				var blist = aList[i].split(",");
				var name = "biaoji_" + blist[0];
				document.getElementById(name).innerHTML = "<img src=" + url + "/image/hongqi.gif class=juzhong width=16  height=16 >";
			}
		}
	});




//	document.getElementById("tihao_5").style.background = "#f1f1f1";

}




