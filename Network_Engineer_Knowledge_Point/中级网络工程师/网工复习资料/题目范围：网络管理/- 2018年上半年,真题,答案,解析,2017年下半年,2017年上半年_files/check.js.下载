function delProductOfCartTS(url,product_id,type,kemu_id){
	if (confirm("您确定要删除吗？"))
	{
		var web_url = url + "/delproductcart.jsp?product_id=" + product_id + "&type=" + type + "&product_kemu_id=" + kemu_id + ""; 
		$.get(web_url, {cache:false},function (data, textStatus){
	//		showcartcnt_caidan(url);
	//		showcartlist_caidan(url);
		//	alert();
			document.getElementById("jiajiagou_list").value = "";



			var f = document.forms["formcart"]; 
			f.action = "cart.jsp";
			f.submit();








	
		});
	}
}


