//单选
function showbiankuang(isshow,ishidden){
//	alert(isshow);
//	alert(ishidden);

	document.getElementById(isshow).className = "plan_xx_biankuang_selected";					
//	alert();
	var aList = ishidden.split("_");
	for (i = 0;i < aList.length ;i ++ )
	{
		if(aList[i] != "")
		{	
			if (aList[i] != isshow)
			{
				document.getElementById(aList[i]).className = "plan_xx_biankuang";					
			}			
		}
	}
}


//复选
//显示边框 同时赋值
function showbiankuang_fuxuan_zice_xml_5(name,v,kemu_id,url,fw){
//	alert(document.getElementById(name).className);
	var vv = "_" + v + "_";
	if (document.getElementById(name).className == "zice_xx_biankuang")//已经是未选中
	{
		//现在是选中
		document.getElementById(name).className = "plan_xx_biankuang_selected";
		parent.document.getElementById("timu").value += vv;

	}else if(document.getElementById(name).className == "plan_xx_biankuang_selected"){//已经是选中
		//现在是未选中
		document.getElementById(name).className = "zice_xx_biankuang";	
		parent.document.getElementById("timu").value = parent.document.getElementById("timu").value.replace(vv,"");

	}

}






//复选
function showbiankuang_fuxuan(isshow){
//	alert(isshow);
	var show = "qbk" + isshow;
	var f = document.forms["form_plan_step2"]; 
//	alert('1');
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="q2_answer" && f.elements[i].checked == true  && f.elements[i].value == isshow)
		{
		//	alert('3');
			document.getElementById(show).className = "plan_xx_biankuang_selected";					
		}
		if (f.elements[i].name=="q2_answer" && f.elements[i].checked == false  && f.elements[i].value == isshow)
		{
		//	alert('3');
			document.getElementById(show).className = "plan_xx_biankuang";					
		}

		
	}
//	alert();

}



function show_fenlei_list_line_user_data(line_list,url){

	var web_url = "tk_fenlei_list_line_user_data.jsp?line_list=" + line_list + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){

		var aList = data.split("|");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				if (aList[i].indexOf("9999999_") == -1)//去头去尾
				{
				//	alert(aList[i]);

					//开始渲染每一个格
					var bList = aList[i].split("_");
					var name_done = "done_" + bList[0];
					if (bList[2] == "null")
					{
						bList[2] = "";
					}
					if (bList[2] == "")
					{
						document.getElementById(name_done).innerHTML = "";
					}else{
						if (bList[2] > 0)
						{
							document.getElementById(name_done).innerHTML = "<img src=image/dui.gif class=juzhong width=19  height=22 >";
						}
					}

					var name_no_right = "no_right_" + bList[0];
					if (bList[3] == "null")
					{
						bList[3] = "";
					}
					if (bList[3] == "")
					{
						document.getElementById(name_no_right).innerHTML = "";					
					}else{
						if (bList[3] == 0)
						{
							document.getElementById(name_no_right).innerHTML = "<img src=image/deng.jpg class=juzhong width=23  height=28 >";					
						}
					}

					



				}

			}
		}


	
	});
//	alert();
}






function show_line_user_data_chongci(line_list,url){
//	alert(line_list);
	var web_url = "tk_index_line_user_data_chongci.jsp?line_list=" + line_list + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){

		var aList = data.split("|");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				if (aList[i].indexOf("9999999_") == -1)//去头去尾
				{
				//	alert(aList[i]);

					//开始渲染每一个格
					var bList = aList[i].split("_");
					var name_done = "done_cnt_" + bList[0];
					if (bList[2] == "null")
					{
						bList[2] = "";
					}
					if (bList[2] == "" || bList[2] == 0)
					{
						document.getElementById(name_done).innerHTML = "";
					}else{
						document.getElementById(name_done).innerHTML = "" + bList[2] + "道";
					}

					var name_wrong = "wrong_cnt_" + bList[0];
					if (bList[3] == "null" || bList[3] == 0)
					{
						bList[3] = "";
					}else{
						bList[3] = "<img src=image/cuo2.gif class=juzhong width=16  height=16 >" + bList[3];					
					
					}
					if (bList[3] == "" || bList[3] == 0)
					{
						document.getElementById(name_wrong).innerHTML = "";					
					}else{
						document.getElementById(name_wrong).innerHTML = "<span class=red_num>" + bList[3] + "道</span>";					
					}

					var name_biaoji = "biaoji_cnt_" + bList[0];
					if (bList[4] == "null"  || bList[4] == 0)
					{
						bList[4] = "";
					}else{
						bList[4] = "<img src=image/hongqi.gif class=juzhong width=16  height=16 >" + bList[4];					
					}
					if (bList[4] == "" || bList[4] == 0)
					{
						document.getElementById(name_biaoji).innerHTML = "";

					}else{
						document.getElementById(name_biaoji).innerHTML = "<span class=bj_num>" + bList[4] + "道</span>";
					
					}





				}

			}
		}


	
	});
//	alert();
}


