//  alert(nn);
 // alert(mm);
  var kk = 0;
  var f = document.forms["form_plan_step2"]; 
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == nn && f.elements[i].value == mm && f.elements[i].checked == true){
		kk = 1;
	}		
  }
//  alert(kk);
  if (kk == 0)
  {
//		alert();
		TheForm.style.background="#FFFFFF";
  }else{
		TheForm.style.background="#A5DCF0";
  }

}


