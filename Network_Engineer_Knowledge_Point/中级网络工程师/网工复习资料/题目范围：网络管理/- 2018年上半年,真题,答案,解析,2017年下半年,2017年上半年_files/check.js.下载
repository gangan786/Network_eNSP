		});

}



function delmywrong(product_id,tihao,paper_id,tixing,kemu_id,url){
	if (confirm("您确定要把此题移出我的错题库吗？"))
	{
		var name = kemu_id+ "_" + paper_id + "_" + tihao + "_" + tixing + ".html";	
		window.location = url + "/dealwithcenter.jsp?action=delmywrong&product_id=" + product_id + "&tihao=" + tihao + "&paper_id=" + paper_id + "&tixing=" + tixing + "&kemu_id=" + kemu_id + "&name=" + name + "";		
	} 
}




function delmywrong_chongci(product_id,tihao,paper_id,tixing,kemu_id,url,shunxu_all,chongci_paper_id){
