function check_email(nn,url){

//	alert(nn);
	if (!chkemail(nn)) {
		document.getElementById("youjian").innerHTML = "&nbsp;&nbsp;<span class=red-text>邮件格式错误！</span>";
		document.getElementById("email_up").value = 0;
		return(false);
	}else{
		document.getElementById("youjian").innerHTML = "";
		document.getElementById("email_up").value = 1;
		return(false);	
	}
}





function check_modpwd_oldpwd(nn,url){

	var web_url = url + "/check_modpwd_oldpwd_xml.jsp?oldpwd=" + nn + ""; 
//	alert(nn);
	$.get(web_url, {cache:false},function (data, textStatus){
		if (data.indexOf("wrong")!= -1)//原密码输入错误
		{
			document.getElementById("show_oldpwd").innerHTML = "&nbsp;&nbsp;<span class=red-text>原密码输入错误！</span>";
			document.getElementById("oldpwd_up").value = 0;
			return(false);
		}else{//原密码输入正确
			document.getElementById("show_oldpwd").innerHTML = "&nbsp;&nbsp;<img src=" + url + "/image/duigou.gif width=13 height=13>";
			document.getElementById("oldpwd_up").value = 1;
			return(true);
		}
	});

//	alert(document.all.form1.username_up.value);
}




function check_verification_code(nn,url){

	var web_url = url + "/check_verification_code_xml.jsp?verification_code=" + nn + ""; 
//	alert(nn);
	if (nn.length == 6) {
		$.get(web_url, {cache:false},function (data, textStatus){
	//		alert(trim(data));
			if (data.indexOf("have") != -1)//有验证码
			{
				document.getElementById("verification_code_area").innerHTML = "&nbsp;&nbsp;<img src=" + url + "/image/duigou.gif width=13 height=13>";
				document.getElementById("verification_code_up").value = 1;
				return(true);				
			}else{//无验证码
				document.getElementById("verification_code_area").innerHTML = "&nbsp;&nbsp;<span class=red-text>验证码无效,请重新获取！</span>";
				document.getElementById("verification_code_up").value = 0;
				return(false);
			}	
		});
	}else{
		document.getElementById("verification_code_area").innerHTML = "&nbsp;&nbsp;<span class=red-text>验证码错误！</span>";
		document.getElementById("verification_code_up").value = 0;
		return(false);	
	}
}






function jiajiagou(product_id,sale_price,yuanshi_price,jiajiagou_list,father_product_id){

	var one = "|" + product_id + "_" + sale_price + "_" + yuanshi_price + "_" + father_product_id;
//	alert(one);

	var button_name = "jiajiagou_" + product_id + "_" + father_product_id;
	var str = jiajiagou_list;

	var f = document.forms["formcart"]; 
	for (i=0;i<f.elements.length;i++){

		if (f.elements[i].name == button_name)
		{
			if (f.elements[i].checked == true)
			{
//				alert("jj");
				str = str + "" + one;
//				alert("jj" + str);

			}else if(f.elements[i].checked == false){
				str = str.replace(one,"");
			}

		}
	}
//	alert(str);

	document.getElementById("jiajiagou_list").value = str;
//	alert(document.getElementById("jiajiagou_list").value);

	f.action = "cart.jsp";
	f.submit();


}







function check_username(nn,url){

	var web_url = url + "/check_same_name_xml.jsp?username=" + nn + ""; 
//	alert(nn);
	if (!chkemail(nn)) {
		document.getElementById("youjian").innerHTML = "&nbsp;&nbsp;<span class=red-text>邮件格式错误！</span>";
	//	document.all.form1.username.focus();
		document.getElementById("username_up").value = 0;
		return(false);
	}else{

		$.get(web_url, {cache:false},function (data, textStatus){
			if (data.indexOf("chongfu")!= -1)//有重复
			{
				document.getElementById("youjian").innerHTML = "&nbsp;&nbsp;<span class=red-text>邮箱已经注册过！</span>";
				document.getElementById("username_up").value = 0;
				return(false);
			}else{
				document.getElementById("youjian").innerHTML = "&nbsp;&nbsp;<img src=" + url + "/image/duigou.gif width=13 height=13>";
				document.getElementById("username_up").value = 1;
				return(true);
			}
		});


	
	}
//	alert(document.all.form1.username_up.value);
}

function check_password(nn,url){

//	var f = document.forms["form_reg"];
//	document.getElementById("mima").innerHTML = "&nbsp;&nbsp;<span class=red-text>密码不能为空！</span>";	
//	alert(document.getElementsByName("mima"));
/*	alert(f.elements.length);
	for (i=0;i<f.elements.length;i++){
		alert("j");
		if (f.elements[i].name=="mima"){
			alert("k");
			f.elements[i].innerHTML = "&nbsp;&nbsp;<span class=red-text>密码不能为空！</span>";		
		}
	}
*/
	if (nn == "")
	{
		document.getElementById("mima").innerHTML = "&nbsp;&nbsp;<span class=red-text>密码不能为空！</span>";
		document.getElementById("password_up").value = 0;
		document.getElementById("password2_up").value = 0;
		document.getElementById("mima2").innerHTML = "";
		return(false);
	}else if(nn.indexOf("=") != -1 || nn.indexOf("%") != -1 || nn.indexOf("*") != -1 || nn.indexOf(";") != -1 || nn.indexOf("'") != -1){
		document.getElementById("mima").innerHTML = "&nbsp;&nbsp;<span class=red-text>密码中含有非法字符！</span>";
		document.getElementById("password_up").value = 0;
		document.getElementById("password2_up").value = 0;
		document.getElementById("mima2").innerHTML = "";
		return(false);
	}else{
		document.getElementById("mima").innerHTML = "&nbsp;&nbsp;<img src=" + url + "/image/duigou.gif width=13 height=13>";
		document.getElementById("password_up").value = 1;
		document.getElementById("password2_up").value = 0;
		document.getElementById("mima2").innerHTML = "";
		return(true);		
	}

}


function check_password2(nn,url){
//	alert(nn);
	var mm = document.getElementById("password1").value;
//	alert(mm);
	if (nn != mm)
	{
		document.getElementById("mima2").innerHTML = "&nbsp;&nbsp;<span class=red-text>两次密码不一致！</span>";
		document.getElementById("password2_up").value = 0;
		return(false);
	}else{
		document.getElementById("mima2").innerHTML = "&nbsp;&nbsp;<img src=" + url + "/image/duigou.gif width=13 height=13>";
		document.getElementById("password2_up").value = 1;
		return(true);		
	}
}

function check_xieyi(){
	var f = document.forms["form_reg"]; 
	var selected = 0;
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="xieyi" && f.elements[i].checked == true)
		{
			selected = 1;
			document.getElementById("xy").innerHTML = "";
		}
	}
	document.getElementById("xieyi_up").value = selected;
}




function chkRegInfoStep1(){
//	alert(document.getElementById("study_kemu").value);
	if (document.getElementById("verification_code_up").value == 1 && document.getElementById("xieyi_up").value == 1 )
	{		
		return(true);
	}else{
		if (document.getElementById("verification_code_up").value == 0)
		{
			document.getElementById("verification_code_area").innerHTML = "&nbsp;&nbsp;<span class=red-text>验证码错误！</span>";
		}
		if (document.getElementById("xieyi_up").value == 0)
		{
			document.getElementById("xy").innerHTML = "&nbsp;&nbsp;<span class=red-text>协议未同意！</span>";
		}
		
		return(false);
	}


}

function chkLoginInfoStep2(){
//	alert(document.getElementById("study_kemu").value);
	if (document.getElementById("verification_code_up").value == 1 )
	{		
		return(true);
	}else{
		if (document.getElementById("verification_code_up").value == 0)
		{
			document.getElementById("verification_code_area").innerHTML = "&nbsp;&nbsp;<span class=red-text>验证码错误！</span>";
		}
				
		return(false);
	}


}



function uppay(){
	document.getElementById('popIframe').style.display='block';
	document.getElementById('loginBox').style.display='block';


	var f = document.forms["formconfirm"]; 
	var selected = 0;
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name=="bank" && f.elements[i].checked == true)
		{
			selected = f.elements[i].value;
		}
	}
//	alert(selected);
	if (selected == 'kuaiqian')
	{
		f.action='billion/send.jsp';
	}else if(selected == 'alipay'){
		f.action='alipaydirect/send.jsp';		
	}

	

	f.submit();
}








function showtupian_forpackageshow_3(product_id,url,product_kemu_id,type){
//	alert();
	var web_url = url + "/showtupian_forproductshow_3.jsp?product_id=" + product_id + "&product_kemu_id=" + product_kemu_id + "&type=" + type + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
	//	alert(data);
		document.getElementById("tupian_show").innerHTML = data;
	//	document.getElementById("datu").innerHTML = path;
	//	show_datu('kemu1/image_yulan/20130308143217_11.jpg');

	});


}



