	});
}



function addinfotousertk(tixing,product_id,paper_id,tihao,url){

	var answer = "";//用户选择的答案
	if(tixing == 'xuanze'){//是选择题，隐藏选项按钮
		
		if(document.getElementById("answer_option_user_a").checked == false && document.getElementById("answer_option_user_b").checked == false && document.getElementById("answer_option_user_c").checked == false && document.getElementById("answer_option_user_d").checked == false){
			//当用户没进行过任何选择的时候，提示并跳出这个函数
			alert("您还没有进行选择");
			return(false);
		}else{
			//当用户进行过选择的时候，隐藏其他选项
			if(document.getElementById("answer_option_user_a").checked == false){
				document.getElementById("answer_option_user_a").style.display = "none";		
			}else{
				answer = "A";			
			}
			if(document.getElementById("answer_option_user_b").checked == false){
				document.getElementById("answer_option_user_b").style.display = "none";		
			}else{
				answer = "B";			
			}
			if(document.getElementById("answer_option_user_c").checked == false){
				document.getElementById("answer_option_user_c").style.display = "none";		
			}else{
				answer = "C";			
			}
			if(document.getElementById("answer_option_user_d").checked == false){
				document.getElementById("answer_option_user_d").style.display = "none";		
			}else{
				answer = "D";			
			}	
			document.getElementById('tk_tjchakanjx').style.display = 'none';
		}

	}

//	alert();
	//取出答案和解析进行显示
	var web_url = url + "/tk_jiexi.jsp?product_id=" + product_id + "&tixing=" + tixing + "&answer=" + answer + "&paper_id=" + paper_id + "&tihao=" + tihao + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
		document.getElementById("jiexi_area").style.display='block';
		document.getElementById("jiexi_area").innerHTML = data;
	});
}




function kt_tiku(text){
	if (confirm("您确定要" + text + "科目的智能真题库吗?"))
	{
		return(true);
	}else{
		return(false);		
	}	
	
}


function kd_readarea2(area1,area2,url){

	if(area1 == 0){//当一级选择，是全部地区（0）的时候，待提交的id为area1
		document.all('kd_area_id').value = area1;
		document.all('kd_showarea2').style.display='none';
//		alert('a');
	}else{//当一级选择，不是全部地区（0）的时候
		document.all('kd_area_id').value = area1;//待提交的id为area1
		document.all('kd_showarea2').style.display='block';
//		alert('b');

		var web_url = url + "/kd_base_area_2_xml.jsp?area1=" + area1 + "&area2=" + area2 + ""; 
		$.get(web_url, {cache:false},function (data, textStatus){
			document.getElementById('kd_showarea2').innerHTML = data;
		});
//		alert();
	}
//	alert(document.all('kd_area_id').value);
}


function kd_uparea2(area2){
//	alert();
//	var area_id_name = 'kd_area_id';
	document.all('kd_area_id').value = area2;	
//	alert(document.all('kd_area_id').value);
}



function readarea2(area1,area2,nn,url){
//	alert();
	var showarea2_name = 'showarea2_' + nn;
	var area_id_name = 'area_id_' + nn;
	if(area1 == 0){//当一级选择，是全部地区（0）的时候，待提交的id为area1
		document.all(area_id_name).value = area1;
		document.all(showarea2_name).style.display='none';
	}else{//当一级选择，不是全部地区（0）的时候
		document.all(area_id_name).value = area1;//待提交的id为area1
		document.all(showarea2_name).style.display='block';

		var web_url = url + "/base_area_2_xml.jsp?area1=" + area1 + "&area2=" + area2 + "&nn=" + nn + ""; 
		$.get(web_url, {cache:false},function (data, textStatus){
			document.getElementById(showarea2_name).innerHTML = data;
		});
//		alert();
	}
//	alert(document.all(area_id_name).value);
}


function uparea2(area2,nn){
	var area_id_name = 'area_id_' + nn;
	document.all(area_id_name).value = area2;	
//	alert(document.all(area_id_name).value);
}


function chkShenqing(nn){
	var area_id_name = 'area_id_' + nn;
	var area_id = document.all(area_id_name).value;

	var address_name = 'address_' + nn;
	var address = document.all(address_name).value;

	var name_name = 'name_' + nn;
	var name = document.all(name_name).value;

	var tel_name = 'tel_' + nn;
	var tel = document.all(tel_name).value;

	var remark_name = 'remark_' + nn;
	var remark = document.all(remark_name).value;
	
	var id_name = 'id_' + nn;
	var id = document.all(id_name).value;

	var product_id_name = 'product_id_' + nn;
	var product_id = document.all(product_id_name).value;

	var kemu_id_name = 'kemu_id_' + nn;
	var kemu_id = document.all(kemu_id_name).value;


	var form_name = 'form_' + nn;
//	alert(document.all(area_id_name).value);
	if (area_id == 0)
	{
		//area_id = 0 没有选择地区
		alert('请填写所属地区');
	//	return(false);
	}else if (address == '')
	{
		alert('请填写详细地址');
	}else if (name == '')
	{
		alert('请填写收货人姓名');
	}else if (tel == '')
	{
		alert('请填写收货人电话');
	}else
	{
	//	alert();
		if (confirm("确认提交您的申领吗？"))
		{
			window.location="dealwithcenter.jsp?action=addshenling&area_id=" + area_id + "&address=" + address + "&name=" + name + "&tel=" + tel + "&remark=" + remark + "&id=" + id + "&product_id=" + product_id + "&kemu_id=" + kemu_id + "";
		}
	//	alert('kk');
	//	return(true);
	}


}




function chkQrsh(nn){


	var form_name = 'form_' + nn;
//	alert(document.all(area_id_name).value);

	if (confirm("确认提交？"))
	{
		window.location="dealwithcenter.jsp?action=qrshshenling&id=" + nn + "";
	}


}
