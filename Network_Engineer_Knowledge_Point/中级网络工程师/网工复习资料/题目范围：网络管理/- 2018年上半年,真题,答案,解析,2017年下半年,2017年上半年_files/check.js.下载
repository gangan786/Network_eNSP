function select_kemu(url,web_url,select_kemu_id,mubiao_url){

	url = url.replace(/&/g,"---");
	url = url.replace("?","___");
//	alert(url);
//	alert("jiji" + web_url);

	var web_url2 = web_url + "/addtrace_select_kemu.jsp?url_save=" + url + "&select_kemu_id=" + select_kemu_id + ""; 
	$.get(web_url2, {cache:false},function (data, textStatus){

		window.location = web_url + "/" + mubiao_url + "";
	});
}

