function showjiexi(product_id,tixing,answer,url,paper_id,answer_option,look_jiexi,tihao){
//	alert(tixing);
	var chuan_tixing = 0;
	if (tixing == 'xuanze')
	{
		chuan_tixing = 1;
	}else if (tixing == 'anli' || tixing == 'lunwen')
	{
		chuan_tixing = 2;
	}
	var web_url = url + "/tk_jiexi.jsp?product_id=" + product_id + "&tixing=" + chuan_tixing + "&answer=" + answer + "&paper_id=" + paper_id + "&look_jiexi=" + look_jiexi + "&tihao=" + tihao + ""; 
//	alert(web_url);
	$.get(web_url, {cache:false},function (data, textStatus){
		document.getElementById("jiexi_area").style.display='block';
		document.getElementById("jiexi_area").innerHTML = data;

		if (answer != null)//有用户答案
		{
			if(answer_option != ''){//有标准答案
				if (answer_option == answer)
				{
					document.getElementById("right_wrong").value = "right";
					document.getElementById("right_wrong_pn").value = "right";
				}else{
					document.getElementById("right_wrong").value = "wrong";	
					document.getElementById("right_wrong_pn").value = "wrong";	
				}
			}else{//没有标准答案
				document.getElementById("right_wrong").value = "no_standard_answer";
				document.getElementById("right_wrong_pn").value = "no_standard_answer";
			}
		}else{//没有作答，直接看解析
		//	alert();		
		}



	//	showdaohang(product_id,url,paper_id,tixing);
	});
	

}

