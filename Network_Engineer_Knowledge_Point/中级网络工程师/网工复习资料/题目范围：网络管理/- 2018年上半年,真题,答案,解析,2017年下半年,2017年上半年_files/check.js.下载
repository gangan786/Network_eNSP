//题目范围的写入 第一次初始化 （与上面函数的区别是 不再调显示页面了 要不然就死循环了）
function step2_timu_value_chushihua_xml_5(kemu_id,url,fw,cnt,time){	
	//题目范围
	document.getElementById("timu").value = '';
	//试题数量
	document.getElementById("cnt").value = cnt;
	//做题时间
	document.getElementById("time").value = time;

}

function show_datu(imgpath){

//	alert(imgpath);

//	document.getElementById("datu").innerHTML = "<img src=" + imgpath + " id=image_datu >";
//	var w = document.getElementById("image_datu").width;
//	var h = document.getElementById("image_datu").height;
//	alert("图片\n宽:"+w+"\n高:"+h)


    var img = document.createElement('img');//创建一个img元素
    img.src = imgpath;//指定src
//	alert(img.width);
	var show_w = 0;
	var show_h = 0;

	if (img.width >= 478 && img.height >= 620)
	{
		if (img.width/478 >= img.height/620 )//倍数大，就证明边大
		{
			show_w = 478;
			show_h = (478*img.height)/img.width;
		}else{
			show_h = 620;
			show_w = (620*img.width)/img.height;		
		}

	}else if(img.width <= 478 && img.height <= 620){
	
		show_w = img.width;
		show_h = img.height;	
	}else if(img.width > 478 && img.height < 620){
		show_w = 478;
		show_h = (478*img.height)/img.width;		
	}else if(img.width < 478 && img.height > 620){
		show_h = 620;
		show_w = (620*img.width)/img.height;
	}

//	alert("<img src=" + imgpath + " width= " + show_w + " height=" + show_h + ">");
	document.getElementById("datu").innerHTML = "<img src=" + imgpath + " width= " + show_w + " height=" + show_h + ">";
// height 470

//    img.style.position="absolute";//防止正常的内容变形
  //  img.style.visibility='hidden';//藏起来
  //  var inj=document.getElementById('box').appendChild(img);//插入到box中。当然插入到document.body也可以
  //  alert('宽：'+inj.offsetWidth);//然后就可以通过 offset 取得宽和高了
  //  alert('高：'+inj.offsetHeight);

}

