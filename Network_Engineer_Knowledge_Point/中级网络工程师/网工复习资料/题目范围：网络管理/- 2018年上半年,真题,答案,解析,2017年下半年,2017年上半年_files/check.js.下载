	$.get(web_url, {cache:false},function (data, textStatus){
	//	alert(data);
		document.getElementById("tupian_show").innerHTML = data;
	//	document.getElementById("datu").innerHTML = path;
	//	show_datu('kemu1/image_yulan/20130308143217_11.jpg');

	});


}

function showtupian_forplanshow(no,product_id,path,url,k,color,product_kemu_id){
//	alert(color);
	var web_url = url + "/showtupian_forplanshow.jsp?no=" + no + "&product_id=" + product_id + "&path=" + path + "&color=" + color + "&product_kemu_id=" + product_kemu_id + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
		var kkk = "tupian_show_" + k;
	//	alert(kkk);

		document.getElementById(kkk).innerHTML = data;
	//	document.getElementById("datu").innerHTML = path;
	//	show_datu('kemu1/image_yulan/20130308143217_11.jpg');

	});


}




function check_if_show_select_fun(url,web_url){
	
	var web_url2 = web_url + "/check_if_show_select.jsp"; 
	$.get(web_url2, {cache:false},function (data, textStatus){
		if (data.indexOf("-show-")!= -1)//符合弹出
		{
			//弹出提示框
			document.getElementById('xianshi').style.display = 'block';
		}

	});


}




function goto_kemu_fun(url,web_url){
	
	//url 本页面是什么
	if (url == "http://www.rkpass.cn/" || url == "http://www.rkpass.cn" ||  url == "http://rkpass.cn" ||  url == "http://rkpass.cn/" ||  url == "www.rkpass.cn" ||  url == "www.rkpass.cn/"  ||  url == "rkpass.cn" ||  url == "rkpass.cn/" )//默认网址加上.html后缀
	{
		//对默认的第一科规范网址，为了后面比较用
		url = "http://www.rkpass.cn/index.html"
	}
	//下面这个是在开发环境中用的
	else if (url == "http://localhost:8080/ruankao_web_version_0103//index.html" || url == "http://localhost:8080/ruankao_web_version_0103/index.html")
	{
		url = "http://localhost:8080/ruankao_web_version_0103/index.html"
	}


	//当用户没有登陆的时候，跳转到这个ip最后一个访问科目去;当登陆了，跳转到这个用户的最后一次访问科目去
	var web_url2 = web_url + "/searchkemu.jsp"; 
	$.get(web_url2, {cache:false},function (data, textStatus){
		if (data.indexOf("--")!= -1)//没有访问记录
		{
			//弹出提示框
			document.getElementById('xianshi').style.display = 'block';
		}else if (data.indexOf("-1-")!= -1)//kemu1
		{
			if (url.indexOf("index.html") == -1)
			{
				window.location = web_url + "/index.html";
			}	
		}else if (data.indexOf("-2-")!= -1)//kemu2
		{
			if (url.indexOf("index2.html") == -1)
			{
				window.location = web_url + "/index2.html";
			}
		}else if (data.indexOf("-3-")!= -1)//kemu3
		{
			if (url.indexOf("index3.html") == -1)
			{
				window.location = web_url + "/index3.html";
			}
		}else if (data.indexOf("-4-")!= -1)//kemu4
		{
			if (url.indexOf("index4.html") == -1)
			{
				window.location = web_url + "/index4.html";
			}
		}else if (data.indexOf("-5-")!= -1)//kemu5
		{
			if (url.indexOf("index5.html") == -1)
			{
				window.location = web_url + "/index5.html";
			}
		}else if (data.indexOf("-6-")!= -1)//kemu6
		{
			if (url.indexOf("index6.html") == -1)
			{
				window.location = web_url + "/index6.html";
			}
		}else if (data.indexOf("-7-")!= -1)//kemu7
		{
			if (url.indexOf("index7.html") == -1)
			{
				window.location = web_url + "/index7.html";
			}
		}else if (data.indexOf("-8-")!= -1)//kemu8
		{
			if (url.indexOf("index8.html") == -1)
			{
				window.location = web_url + "/index8.html";
			}
		}else if (data.indexOf("-9-")!= -1)//kemu9
		{
			if (url.indexOf("index9.html") == -1)
			{
				window.location = web_url + "/index9.html";
			}
		}else if (data.indexOf("-10-")!= -1)//kemu10
		{
			if (url.indexOf("index10.html") == -1)
			{
				window.location = web_url + "/index10.html";
			}
		}else if (data.indexOf("-11-")!= -1)//kemu11
		{
			if (url.indexOf("index11.html") == -1)
			{
				window.location = web_url + "/index11.html";
			}
		}else if (data.indexOf("-12-")!= -1)//kemu12
		{
			if (url.indexOf("index12.html") == -1)
			{
				window.location = web_url + "/index12.html";
			}
		}else if (data.indexOf("-13-")!= -1)//kemu13
		{
			if (url.indexOf("index13.html") == -1)
			{
				window.location = web_url + "/index13.html";
			}
		}else if (data.indexOf("-14-")!= -1)//kemu14
		{
			if (url.indexOf("index14.html") == -1)
			{
				window.location = web_url + "/index14.html";
			}
		}else if (data.indexOf("-15-")!= -1)//kemu15
		{
			if (url.indexOf("index15.html") == -1)
			{
				window.location = web_url + "/index15.html";
			}
		}else if (data.indexOf("-16-")!= -1)//kemu16
		{
			if (url.indexOf("index16.html") == -1)
			{
				window.location = web_url + "/index16.html";
			}
		}else if (data.indexOf("-17-")!= -1)//kemu17
		{
			if (url.indexOf("index17.html") == -1)
			{
				window.location = web_url + "/index17.html";
			}
		}

	});


	//当用户登陆之后，跳转到session中的科目去

}

