				if(s_b){
					document.getElementById(name_b).style.background = "#e4e4e4";
				}								



			}
		}


		//做错
		var aalist = type_str[1].split(";");
		for (i = 0;i < aalist.length ;i ++ )
		{
			if(aalist[i] != "")
			{	
				var bblist = aalist[i].split(",");
