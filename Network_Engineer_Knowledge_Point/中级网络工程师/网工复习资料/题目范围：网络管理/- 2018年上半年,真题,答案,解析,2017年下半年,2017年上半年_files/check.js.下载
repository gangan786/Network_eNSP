	{
		document.getElementById("message").innerHTML = "<img src=" + url + "/image/close2.gif  width=15 height=15 class=juzhong>&nbsp;&nbsp;<span class=red-text>请填写用户名！</span>";		
		return(false);
	}else if(document.getElementById("password").value == ""){
		document.getElementById("message").innerHTML = "<img src=" + url + "/image/close2.gif  width=15 height=15 class=juzhong>&nbsp;&nbsp;<span class=red-text>请填写密码！</span>";		
		return(false);
	}else{
	//	alert('hu');
		return(true);	
	}
}





function chkRegInfoForEnterprise(){
	if (document.getElementById("username_up").value == 1 && document.getElementById("password_up").value == 1 && document.getElementById("password2_up").value == 1 &&  document.getElementById("study_kemu").value != '请选择')
	{
		
		document.getElementById("bkkm").innerHTML = "";
		return(true);
	}else{
		if (document.getElementById("username_up").value == 0)
		{
			document.getElementById("youjian").innerHTML = "&nbsp;&nbsp;<span class=red-text>邮箱有误！</span>";
		}
		if (document.getElementById("password_up").value == 0)
		{
			document.getElementById("mima").innerHTML = "&nbsp;&nbsp;<span class=red-text>密码有误！</span>";
		}
		if (document.getElementById("password2_up").value == 0)
		{
			document.getElementById("mima2").innerHTML = "&nbsp;&nbsp;<span class=red-text>密码有误！</span>";
		}
		if (document.getElementById("study_kemu").value == '请选择')
		{
			document.getElementById("bkkm").innerHTML = "&nbsp;&nbsp;<span class=red-text>请选择！</span>";
		}

		return(false);
	}


}







