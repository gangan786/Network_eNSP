	{
		var name = kemu_id+ "_" + paper_id + "_" + tihao + "_" + tixing + "_" + chongci_paper_id + "_" + shunxu_all + ".html";	
		window.location = url + "/dealwithcenter.jsp?action=delmywrong&product_id=" + product_id + "&tihao=" + tihao + "&paper_id=" + paper_id + "&tixing=" + tixing + "&kemu_id=" + kemu_id + "&name=" + name + "";		
	} 
}







