function chongci_timu_jump(timu,url,paper_id,kemu_id,canuse_one_kemu){

//	alert();
	var web_url = url + "/chongci_timu_check_power.jsp?paper_id=" + paper_id + "&kemu_id=" + kemu_id + "&canuse_one_kemu=" + canuse_one_kemu + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
		
//		alert(data);
		if (data.indexOf("bunengyong") != -1)//有验证码
		{
			alert("如需要，请开通");
		}else{
			window.location = url + "/tk_timu/" + timu;
		}
	});		
}



function confirm_use(time,order_id,product_id){
	if (confirm("您确定使用吗？"))
	{
		document.getElementById("time").value = time;
		document.getElementById("order_id").value = order_id;
		document.getElementById("product_id").value = product_id;
		

		form_mydocuments.submit();
	}
}


