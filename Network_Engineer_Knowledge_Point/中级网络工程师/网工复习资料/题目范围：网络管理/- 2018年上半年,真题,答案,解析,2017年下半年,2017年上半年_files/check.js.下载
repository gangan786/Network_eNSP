function share_done(nn){

	if (confirm("我已分享完成？"))
	{
	//	form1.submit();

		var share_mudi_web = document.getElementById("share_mudi_web").value;
		var order_id = document.getElementById("order_id").value;
	//	alert(order_id);
		window.location="dealwithcenter.jsp?action=share&type=" + nn + "&share_mudi_web=" + share_mudi_web + "&order_id=" + order_id + "";

	}else{
	
	
	}

//	document.getElementById("share_div").style.display = "block";
//	document.getElementById("share_mudi_web").value = pingtai_name;

}






function addfavorite(sURL, sTitle)
{
    try
    {
        window.external.addFavorite(sURL, sTitle);
    }
    catch (e)
    {
        try
        {
            window.sidebar.addPanel(sTitle, sURL, "");
        }
        catch (e)
        {
            alert("加入收藏失败，请使用Ctrl+D进行添加");
        }
    }
}




function show_tihao_user_chongci(paper_id,url){


	//显示做每个题的次数
	web_url = url + "/tk_paper_tihao_show_chongci.jsp?paper_id=" + paper_id + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
		data = data.replace(/[ ]/g,"");//去空格
		data = data.replace(/[\r\n]/g,"");//去回车
		var type_str = data.split("|");



		//做过
		var aList = type_str[0].split(";");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				var blist = aList[i].split(",");
				var name = "done_" + blist[0];
				var s=document.getElementById(name);//判断这个id的div是否存在
				if(s){
					document.getElementById(name).innerHTML = "" + blist[1] + "&nbsp;";
				}

				var name_b = "tihao_" + blist[0];
				var s_b=document.getElementById(name_b);//判断这个id的div是否存在
				if(s_b){
					document.getElementById(name_b).style.background = "#e4e4e4";
				}								



			}
		}


		//做错
		var aalist = type_str[1].split(";");
		for (i = 0;i < aalist.length ;i ++ )
		{
			if(aalist[i] != "")
			{	
				var bblist = aalist[i].split(",");
				var name = "wrong_" + bblist[0];
				var s=document.getElementById(name);//判断这个id的div是否存在
				if(s){
					document.getElementById(name).innerHTML = "<img src=" + url + "/image/cuo2.gif class=juzhong width=16  height=16 >";
				}
			}
		}



		//标记
		var aaalist = type_str[2].split(";");
		for (i = 0;i < aaalist.length ;i ++ )
		{
			if(aaalist[i] != "")
			{	
				var bbblist = aaalist[i].split(",");
				var name = "biaoji_" + bbblist[0];
				var s=document.getElementById(name);//判断这个id的div是否存在
				if(s){
					document.getElementById(name).innerHTML = "<img src=" + url + "/image/hongqi.gif class=juzhong width=16  height=16 >";
				}
			}
		}


	});

	

	

}



function show_tihao_user(paper_id,url){


	//显示做每个题的次数
	web_url = url + "/tk_paper_tihao_show.jsp?paper_id=" + paper_id + ""; 
	$.get(web_url, {cache:false},function (data, textStatus){
	//	alert(data);
		data = data.replace(/[ ]/g,"");//去空格
		data = data.replace(/[\r\n]/g,"");//去回车
		var type_str = data.split("|");



		//做过
		var aList = type_str[0].split(";");
		for (i = 0;i < aList.length ;i ++ )
		{
			if(aList[i] != "")
			{	
				var blist = aList[i].split(",");
				var name = "done_" + blist[0];
				var s=document.getElementById(name);//判断这个id的div是否存在
