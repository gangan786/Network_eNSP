function user_houxv(url,sheet_question,sheet_list,product_id,answer_time,zice_paper_id){
//	alert(sheet_question);  //20130321232123|0|0|7  product_id|是否做过|是否标记|题号
//	alert(sheet_list); //20130321232123|0|0|7_//20130321232120|0|0|8_//20130321232123|0|0|9
//	alert(product_id);
//	alert(answer_time);




	var f = document.forms["form_zice_paper"]; 
	for (i=0;i<f.elements.length;i++){
		if (f.elements[i].name == "houxv"){
			if (f.elements[i].value == product_id)
			{
				//对这一个复选框开始处理
				var answer_time_str = answer_time.split(" ");
				var shi = parseInt(answer_time_str[0]);
				var fen = parseInt(answer_time_str[1]);
				var miao = parseInt(answer_time_str[2]);
				var answer_time_haomiao = parseInt(answer_time_str[0]) * 3600 + parseInt(answer_time_str[1]) * 60 + parseInt(answer_time_str[2]);

				var sheet_question_str = sheet_question.split("|");
				var new_sheet_question_str = "";//
				var houxv_true = 0;
				if (f.elements[i].checked == true)
				{
				//	alert("ok");
					houxv_true = 1;
					new_sheet_question_str = sheet_question_str[0] + "|" + sheet_question_str[1] + "|1|" + sheet_question_str[3];//打标识
				}else{
					houxv_true = 0;
					new_sheet_question_str = sheet_question_str[0] + "|" + sheet_question_str[1] + "|0|" + sheet_question_str[3];//取消标识
				}

				//更新到数据库
				var web_url = url + "/tk_zice_answer_updata_houxv_xml.jsp?zice_paper_id=" + zice_paper_id + "&product_id=" + sheet_question_str[0] + "&houxv=" + houxv_true + "&answer_time=" + answer_time_haomiao + ""; 
			//	alert();
				$.get(web_url, {cache:false},function (data, textStatus){
					//更新页面
					var new_sheet_list = sheet_list.replace(sheet_question,new_sheet_question_str);
					document.getElementById("sheet_list").value = new_sheet_list;
					var new_sheet_question_product_id = "sheet_question_" + sheet_question_str[0];
					document.getElementById(new_sheet_question_product_id).value = new_sheet_question_str;

					//更新答题卡
					shou_datizhi(new_sheet_list);

				
				});


			}
		}		
	}


}




function user_selected(url,sheet_question,sheet_list,user_answer,answer_time,zice_paper_id){
//	alert(sheet_question);  //20130321232123|0|0|7  product_id|是否做过|是否标记|题号
//	alert(sheet_list); //20130321232123|0|0|7_//20130321232120|0|0|8_//20130321232123|0|0|9
//	alert(user_answer);
//	alert(answer_time);

	var sheet_question_str = sheet_question.split("|");
	var new_sheet_question_str = sheet_question_str[0] + "|1|" + sheet_question_str[2] + "|" + sheet_question_str[3];//标识做过了
//	alert(sheet_question_str[0]);
	var answer_time_str = answer_time.split(" ");
	var shi = parseInt(answer_time_str[0]);
	var fen = parseInt(answer_time_str[1]);
	var miao = parseInt(answer_time_str[2]);
	var answer_time_haomiao = parseInt(answer_time_str[0]) * 3600 + parseInt(answer_time_str[1]) * 60 + parseInt(answer_time_str[2]);

//	alert(shi + "_" + fen + "_" + miao + "_" + answer_time_haomiao);




	//更新到数据库
	var web_url = url + "/tk_zice_answer_updata_xml.jsp?zice_paper_id=" + zice_paper_id + "&product_id=" + sheet_question_str[0] + "&user_answer=" + user_answer + "&answer_time=" + answer_time_haomiao + ""; 
//	alert();
	$.get(web_url, {cache:false},function (data, textStatus){
		//更新页面
	//	alert();
		var new_sheet_list = sheet_list.replace(sheet_question,new_sheet_question_str);
		document.getElementById("sheet_list").value = new_sheet_list;
		var new_sheet_question_product_id = "sheet_question_" + sheet_question_str[0];
		document.getElementById(new_sheet_question_product_id).value = new_sheet_question_str;

		//更新答题卡
		shou_datizhi(new_sheet_list);

	
	});
	
	
	

}







