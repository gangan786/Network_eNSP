function ref_zengzhi_duanxin(nn){
	//nn=1的时候，是选中的，传过来的意思是取消选中
	//nn=2的时候，是没选中的，传过来的意思是选中
	
	if (nn == 1)
	{
		document.getElementById("zengzhi_tel").value = 0;
	}else if(nn == 2){
		document.getElementById("zengzhi_tel").value = 1;
	}

	show_zengzhi();

}

function addcart(product_id,type,num,url,kemu_id){

