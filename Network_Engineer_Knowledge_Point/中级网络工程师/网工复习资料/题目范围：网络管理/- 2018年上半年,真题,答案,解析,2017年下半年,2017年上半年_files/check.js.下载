function selectall(){


  var f = document.forms["form_list"]; 
  var checked = 0;
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == "all"){
		if (f.elements[i].checked == true)
		{
			checked = 1;			
		}
	}		
  }
 
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == "one_product_id"){
		if (checked == 1)
		{
			f.elements[i].checked = true; 
		}else{
			f.elements[i].checked = false; 
		
		}
	}		
  }

}




function ci_show(mm,nn){
	//	alert(nn);
	//	alert(mm);
	//mm 词标签列表
	//nn 需要显示的词
	
	var k = 0;
	var aList = mm.split(",");
	for (i = 0;i < aList.length ;i ++ )
	{
		if(aList[i] != "")
		{	
			//将词标签设置为未选状态
			var name_done_title = "ci_title_" + aList[i];
			document.getElementById(name_done_title).style.background="#f6f6f6";
			document.getElementById(name_done_title).style.color="#000000";
			
			//将词区域隐藏
			var name_done = "ci_detail_" + aList[i];
			document.getElementById(name_done).style.display = 'none';

			k++;
		}
	}

	if ( k>0 )//说明有本次显示有词标签
	{
		//针对'全部'这个状态词标签
		//将词标签设置为未选状态
		var name_done_title = "ci_title_30";
		document.getElementById(name_done_title).style.background="#f6f6f6";
		document.getElementById(name_done_title).style.color="#000000";
		
		//将词区域隐藏
		var name_done = "ci_detail_30";
		document.getElementById(name_done).style.display = 'none';

	}else{//本次显示没有词标签
		//不操作，因为页面没有全部这个标签，操作就会报错
	
	}

	//显示需要显示的词标签
	var display_name_done_title = "ci_title_" + nn;
	document.getElementById(display_name_done_title).style.background="#e3393c";
	document.getElementById(display_name_done_title).style.color="#ffffff";

	//显示需要显示的词区域
	var display_name_done = "ci_detail_" + nn;
	document.getElementById(display_name_done).style.display = 'block';
				


}



function plan1click(nn,mm,TheForm,uu){
//  alert(nn);
//  alert(mm);

 // alert(uu);
  if (uu == "plan1area11")
  {
	  document.getElementById("plan1area12").style.background="#FFFFFF";
	  document.getElementById("plan1area13").style.background="#FFFFFF";
  }else if(uu == "plan1area12"){
	  document.getElementById("plan1area11").style.background="#FFFFFF";
	  document.getElementById("plan1area13").style.background="#FFFFFF";
  }else if(uu == "plan1area13"){
	  document.getElementById("plan1area11").style.background="#FFFFFF";
	  document.getElementById("plan1area12").style.background="#FFFFFF";
  }



  var f = document.forms["form_plan_step1"]; 
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == nn && f.elements[i].value == mm){
		f.elements[i].checked = true; 
		TheForm.style.background="#99D482";
	}		
  }

}

function plan1out(nn,mm,TheForm){
//  alert(nn);
 // alert(mm);
  var kk = 0;
  var f = document.forms["form_plan_step1"]; 
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == nn && f.elements[i].value == mm && f.elements[i].checked == true){
		kk = 1;
	}		
  }
//  alert(kk);
  if (kk == 0)
  {
//		alert();
		TheForm.style.background="#FFFFFF";
  }else{
		TheForm.style.background="#99D482";
  }

}

function plan2click(nn,mm,TheForm,uu){
//  alert(nn);
//  alert(mm);

 // alert(uu);
  if (uu == "plan2area11")
  {
	  document.getElementById("plan2area12").style.background="#FFFFFF";
	  document.getElementById("plan2area13").style.background="#FFFFFF";
  }else if(uu == "plan2area12"){
	  document.getElementById("plan2area11").style.background="#FFFFFF";
	  document.getElementById("plan2area13").style.background="#FFFFFF";
  }else if(uu == "plan2area13"){
	  document.getElementById("plan2area11").style.background="#FFFFFF";
	  document.getElementById("plan2area12").style.background="#FFFFFF";
  }



  var f = document.forms["form_plan_step2"]; 
  for (i=0;i<f.elements.length;i++){
    if (f.elements[i].name == nn && f.elements[i].value == mm){
		f.elements[i].checked = true; 
		TheForm.style.background="#A5DCF0";
	}		
  }

}

